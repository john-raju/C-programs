#include <stdio.h>

int main() {
    int i, j, n;
    
    // Read in the number of rows
    scanf("%d", &n);
    
    // Print the pattern
    for (i = 0; i < n; i++) {
        for (j = 0; j <= i; j++) {
            printf("%c ", 'A' + j);
        }
        printf("\n");
    }
    
    return 0;
}
